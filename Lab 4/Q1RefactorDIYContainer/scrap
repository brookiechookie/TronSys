    int NumberOfItems = 0;
    // Create a new list which contains references to the items
    DoublyLinkedList<float> floatList;
    float a = 1.987;
    floatList.push_back( a );
    NumberOfItems = floatList.Size();
    std::cout<< "[Main]: Number of items in the list = " << NumberOfItems <<
    std::endl;
    floatList.printList();

    a++;
    floatList.push_back( a );
    NumberOfItems = floatList.Size();
    std::cout<< "[Main]: Number of items in the list = " << NumberOfItems <<
    std::endl;
    floatList.printList();

    aumberOfItems = floatList.Size();
    std::cout<< "[Main]: Number of items in the list = " << NumberOfItems <<
    std::endl;
    floatList.printList();

    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );++;
    floatList.push_back( a );
    NumberOfItems = floatList.Size();
    std::cout<< "[Main]: Number of items in the list = " << NumberOfItems <<
    std::endl;
    floatList.printList();

    a++;
    floatList.push_back( a );
    NumberOfItems = floatList.Size();
    std::cout<< "[Main]: Number of items in the list = " << NumberOfItems <<
    std::endl;
    floatList.printList();

    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );
    floatList.pop_front( );
